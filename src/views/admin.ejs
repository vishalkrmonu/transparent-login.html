<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Page</title>
</head>
<body>
    <h1>Admin Page</h1>
    <div class="data-container">
        <% data.forEach((item) => { %>
            <div class="data-item">
                <p>Email: <%= item.email %></p>
                <p>Password: <%= item.password %></p>
            </div>
        <% }); %>
    </div>
</body>
</html>
